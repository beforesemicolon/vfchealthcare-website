{
  "name": "vfchealthcare-website",
  "version": "1.0.0",
  "description": "",
  "main": "build.js",
  "type": "module",
  "scripts": {
    "build": "node build.js",
    "build:watch": "nodemon --watch templates --watch widgets --watch components --watch scripts --watch stylesheets --watch assets --ext 'js,css,html,json' --exec \"NODE_ENV=development npm run build\"",
    "start": "serve public",
    "local": "NODE_ENV=development netlify dev",
    "deploy": "npm run build && netlify deploy --dir=public",
    "deploy:prod": "npm run build && netlify deploy --prod --dir=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beforesemicolon/vfchealthcare-website.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beforesemicolon/vfchealthcare-website/issues"
  },
  "homepage": "https://github.com/beforesemicolon/vfchealthcare-website#readme",
  "dependencies": {
    "@beforesemicolon/site-builder": "^0.39.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.0",
    "serve": "^14.2.3"
  }
}
